{
  "name": "react-highlight.js",
  "description": "A lightweight React wrapper around the Highlight.js syntaxt highlighting library",
  "author": "Brian Vaughn",
  "user": "bvaughn",
  "version": "0.0.1",
  "scripts": {
    "start": "webpack-dev-server --port $PORT",
    "test": "karma start",
    "tdd": "karma start --auto-watch --no-single-run",
    "gh-pages": "webpack",
    "deploy-gh-pages": "node ./lib/deploy_gh_pages.js",
    "dist": "webpack --display-error-details",
    "dist-min": "webpack",
    "dist-modules": "babel ./source --out-dir ./dist-modules",
    "lint": "standard",
    "preversion": "npm run test && npm run lint && npm run dist && npm run dist-min && git commit --allow-empty -am \"Update dist\"",
    "prepublish": "npm run dist-modules",
    "postpublish": "npm run gh-pages && npm run deploy-gh-pages",
    "postinstall": "node lib/post_install.js"
  },
  "main": "dist-modules",
  "dependencies": {
    "highlight.js": "^8.9.1",
    "object.values": "^1.0.3",
    "react": "^0.14.0",
    "react-dom": "^0.14.0",
    "react-pure-render": "1.0.2"
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "dist",
      "karma.conf.js",
      "lib",
      "webpack.config.babel.js"
    ],
    "global": [
      "__DEVTOOLS__",
      "after",
      "afterEach",
      "before",
      "beforeEach",
      "cancelAnimationFrame",
      "describe",
      "it",
      "requestAnimationFrame"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "standard",
      "standard-react"
    ],
    "globals": {
      "describe": false,
      "it": false
    }
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-core": "^5.8.25",
    "babel-eslint": "^4.1.3",
    "babel-loader": "^5.3.2",
    "babel-plugin-react-transform": "^1.1.1",
    "chai": "^3.3.0",
    "clean-webpack-plugin": "^0.1.3",
    "css-loader": "^0.19.0",
    "eslint": "^1.6.0",
    "eslint-loader": "^1.1.0",
    "eslint-plugin-react": "^3.5.1",
    "expect.js": "^0.3.1",
    "extract-text-webpack-plugin": "^0.8.2",
    "file-loader": "^0.8.4",
    "gh-pages": "^0.4.0",
    "git-prepush-hook": "^1.0.1",
    "highlight.js": "^8.8.0",
    "html-webpack-plugin": "^1.6.2",
    "isparta-instrumenter-loader": "^0.2.1",
    "json-loader": "^0.5.3",
    "karma": "^0.13.10",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^0.5.2",
    "karma-mocha": "^0.2.0",
    "karma-phantomjs-launcher": "^0.2.1",
    "karma-sourcemap-loader": "^0.3.5",
    "karma-spec-reporter": "0.0.20",
    "karma-webpack": "^1.7.0",
    "link-package": "^0.1.0",
    "markdown-to-react-components": "bebraw/markdown-to-react-components#highlight",
    "mocha": "^2.3.3",
    "phantomjs": "^1.9.18",
    "phantomjs-polyfill": "0.0.1",
    "purecss": "^0.6.0",
    "raw-loader": "^0.5.1",
    "react-addons-test-utils": "^0.14.0",
    "react-ghfork": "^0.3.2",
    "react-transform-hmr": "^1.0.1",
    "standard": "^5.3.1",
    "style-loader": "^0.12.4",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.12.0",
    "webpack-merge": "^0.2.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bvaughn/react-highlight.js.git"
  },
  "homepage": "https://github.com/bvaughn/react-highlight.js/",
  "bugs": {
    "url": "https://github.com/bvaughn/react-highlight.js/issues"
  },
  "keywords": [
    "react",
    "reactjs",
    "highlight",
    "highlight.js",
    "syntax",
    "highlighting"
  ],
  "license": "MIT",
  "pre-push": []
}
